@model List<todo_api.Models.Domain.Employee>

@{
}
<h1>Employees</h1>

<table class="table"> 
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
            Name
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
 @foreach(var employee in Model)
        {
          <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>
                    <a href="Employees/View/@employee.Id">View</a>
                </td>
            </tr>
        }
    </tbody>
</table>